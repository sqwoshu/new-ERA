    def auto_like_in(self):
        browser = self.browser
        index_in = self.index_in
        index_click = self.index_click

        browser.get('https://bosslike.ru/tasks/instagram/like/')
        window_before = browser.window_handles[0]

        time.sleep(2)
        ballans = browser.find_element(By.ID,'user_points_balance').text
        print('ballans_open', ballans)

        count = browser.find_element(By.XPATH,'//*[@id="list-3-1"]/span[1]').text
        if int(count) >= 20:
            for i in range(20):
                time.sleep(1)
                try:
                    self.click()

                    del index_click[0]

                    self.like_in()

                    browser.switch_to.window(window_before)
                    try:
                        time.sleep(1.5)
                        browser.find_element(By.XPATH,f'//*[@id="task-item-{index_in[0]}"]/div/div/div/div[4]/a').click()

                        ms = WebDriverWait(browser, 15).until(
                            EC.presence_of_element_located((By.CSS_SELECTOR, ".notify-message")))
                        time.sleep(random.randrange(1,3))
                        ms.click()
                        del index_in[0]
                    except Exception:
                        del index_in[0]

                    time.sleep(random.randrange(2,4))
                except Exception:
                    index_click.clear()
                    index_in.clear()
                    self.sleep()
                if i == 19:
                    index_click.clear()
                    index_in.clear()
                    self.sleep()
        else:
            index_click.clear()
            index_in.clear()
            self.sleep()

    def like_in(self):
        browser = self.browser
        try:
            window_after = browser.window_handles[1]
            browser.switch_to.window(window_after)

            try:
                like_in = WebDriverWait(browser, 30).until(EC.presence_of_element_located(
                    (By.XPATH, '//*[@id="react-root"]/section/main/div/div[1]/article/div/div[2]/div/div[2]/section[1]/span[1]')))
                time.sleep(random.randrange(2,3))
                like_in.click()
                time.sleep(random.randrange(1,3))
                browser.close()
            except Exception:
                browser.close()
        except Exception:
            return True
